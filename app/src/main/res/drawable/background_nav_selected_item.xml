<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <stroke
            android:width="@dimen/_20sdp"
            android:color="#000000" />
    </shape>
</item>

<item
    android:bottom="@dimen/_2sdp"
